{
  "multipart": [
    {
      "apply": {
        "model": "beachparty:block/sandcastle_base"
      }
    },
    {
      "apply": {
        "model": "beachparty:block/sandcastle_tower1"
      },
      "when": {
        "tall": "true"
      }
    },
    {
      "apply": {
        "model": "beachparty:block/sandcastle_tower3"
      },
      "when": {
        "right": "true"
      }
    },
    {
      "apply": {
        "model": "beachparty:block/sandcastle_tower2"
      },
      "when": {
        "top": "true"
      }
    },
    {
      "apply": {
        "model": "beachparty:block/sandcastle_tower4"
      },
      "when": {
        "left": "true"
      }
    }
  ]
}