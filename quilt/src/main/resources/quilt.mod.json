{
  "schema_version": 1,
  "mixin": [
    "beachparty.mixins.json",
    "beachparty-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "beachparty",
    "version": "${version}",
    "name": "Beachparty",
    "description": "Take a break - build sandcastles, drink cocktails and enjoy your life.",
    "authors": ["Satisfyu", "BaumeisterJo", "Cristelknight"],
    "contact": {
      "sources": "https://github.com/architectury/architectury-templates"
    },
    "license": "Insert License Here",
    "icon": "assets/examplemod/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "satisfyu.beachparty.quilt.BeachpartyQuilt"
      ],
      "client": [
        "satisfyu.beachparty.fabriclike.client.BeachpartyFabricLikeClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.4"
      },
      {
        "id": "architectury",
        "version": ">=8.1.79"
      }
    ]
  }
}